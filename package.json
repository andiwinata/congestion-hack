{
  "name": "congestion-hack",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.18.0",
    "bulma": "^0.6.2",
    "bulma-extensions": "^1.0.13",
    "highcharts": "^6.0.7",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-form": "^3.4.2",
    "react-redux": "^5.0.7",
    "react-scripts": "1.1.1",
    "react-select": "^1.2.1",
    "recompose": "^0.26.0",
    "redux": "^3.7.2",
    "redux-devtools-extension": "^2.13.2",
    "redux-thunk": "^2.2.0"
  },
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "build-css": "node-sass-chokidar src/ -o src/ --include-path ./node_modules src",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive --include-path ./node_modules src/",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css build-js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "gh-pages": "^1.1.0",
    "node-sass-chokidar": "^1.2.0",
    "npm-run-all": "^4.1.2"
  },
  "homepage": "https://andiwinata.github.io/congestion-hack"
}
